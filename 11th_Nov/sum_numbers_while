public class sum_of_numbers {
    public static void main(String args[])
    {
        int sum=0;
        int i=1;
        
        while (i<=10)
        {
            System.out.println(i);
            sum=sum+i;
            i+=1;
        }
        System.out.println("SUm of numbers is:"+sum);
    }

}

