import java.util.Scanner;

public class facto {
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter number:");
        int num=sc.nextInt();
        int facto=1,i=1;

        while (i<=num)
        {
            facto*=i;
            System.out.println(i);
            i++;
        }
        System.out.println(facto);
    }
}

